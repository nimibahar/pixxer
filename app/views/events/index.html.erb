<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>
<div class="container">
  <div class="row">
   <% @events.each do |event| %>
   <div class="col-xs-12 col-md-4">
    <%= link_to event_path(event) do %>
    <div class="card small" >
      <div class="card-category"><%= event.starts_at %></div>
      <div class="card-description"> <%= event.club_id %> </div>
    </div>
    <% end %>
  </div>
  <% end %>
</div>
</div>
